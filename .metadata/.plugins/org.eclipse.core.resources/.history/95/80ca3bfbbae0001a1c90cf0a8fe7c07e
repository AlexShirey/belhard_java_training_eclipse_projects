package Interface;


public class Main {
    public static void main(String[] args){

        Matrix matrix1 = new Matrix();
        matrix1.createMatrix();
        matrix1.fillMatrix();
        matrix1.show();

        Matrix matrix2 = new Matrix();
        matrix2.createMatrix();
        matrix2.fillMatrix();
        matrix2.show();

        System.out.println("Сравнение матриц: " + matrix1.equals(matrix2));
        matrix1.summ(matrix2);

        System.out.println("Вывод числа по номеру ячейки:");
        matrix1.showOne();

        System.out.println("Поэлементоне деление матрицы на число:");
        matrix1.delenie(3);
        matrix1.show();


        Strings string1 = new Strings();
        string1.create();

        string1.showOne();

        Strings string2 = new Strings();
        string2.create();

        System.out.println("Сравнение строк: " + string1.equals(string2));

        System.out.print("Сумма строк:");
        string1.summ(string2);



        Vectors vect1 = new Vectors();
        vect1.create();

        Vectors vect2 = new Vectors();
        vect2.create();

        System.out.println("Сравнение длин двух векторов: " + vect1.SravnenieDlin(vect2));

        System.out.println("Сравнение двух векторов: " + vect1.equals(vect2));

        System.out.println("Сумма двух векторов: ");
        vect1.summ(vect2);








    }


}